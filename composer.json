{
    "name": "krasnopyorovalex/yandex-turbo-pages",
    "description": "simple tool for build yandex turbo pages",
    "keywords": [
        "krasnopyorovalex",
        "yandex-turbo-pages"
    ],
    "homepage": "https://github.com/krasnopyorovalex/yandex-turbo-pages",
    "license": "MIT",
    "authors": [
        {
            "name": "Krasnopyorov Alex",
            "email": "krasnopyorovalex88@gmail.com",
            "homepage": "https://krasber.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Krasnopyorovalex\\YandexTurboPages\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krasnopyorovalex\\YandexTurboPages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
